<keymap version="1" name="Windows copy Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorCreateRectangularSelectionOnMouseDrag">
    <mouse-shortcut keystroke="alt button1" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="EditorPasteFromX11" />
  <action id="FindInPath">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="shift ctrl f" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="f2" />
  </action>
  <action id="RevealIn">
    <keyboard-shortcut first-keystroke="f3" />
  </action>
  <action id="ShowIntentionActions">
    <keyboard-shortcut first-keystroke="alt enter" />
    <mouse-shortcut keystroke="button2" />
  </action>
  <action id="Vcs.ReformatCommitMessage">
    <keyboard-shortcut first-keystroke="shift ctrl f" />
  </action>
  <action id="ViewMainMenu">
    <keyboard-shortcut first-keystroke="shift ctrl alt comma" />
  </action>
</keymap>